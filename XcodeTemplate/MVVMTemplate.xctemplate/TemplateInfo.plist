<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Kind</key>
    <string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>
    <key>Summary</key>
    <string>MVVM Feature with Clean Architecture</string>
    <key>Description</key>
    <string>Creates View, ViewModel, UseCase, Builder, and Router files</string>
    <key>Platforms</key>
    <array>
        <string>com.apple.platform.iphoneos</string>
    </array>
    <key>Options</key>
    <array>
        <dict>
            <key>Identifier</key>
            <string>productName</string>
            <key>Required</key>
            <true/>
            <key>Name</key>
            <string>Feature Name:</string>
            <key>Description</key>
            <string>The name of your feature (e.g., About, Profile, Chat)</string>
            <key>Type</key>
            <string>text</string>
            <key>Default</key>
            <string>Feature</string>
            <key>NotPersisted</key>
            <false/>
        </dict>
    </array>
    <key>Definitions</key>
    <dict>
        <key>camelCasedProductName</key>
        <string>___VARIABLE_productName:identifier___</string>
        <key>coreName</key>
        <string>Core</string>
    </dict>
</dict>
</plist>
